{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "popper.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "datatables@1.10.18",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "media/css/jquery.dataTables.css",
        "media/css/jquery.dataTables.min.css",
        "media/images/favicon.ico",
        "media/images/Sorting icons.psd",
        "media/images/sort_asc.png",
        "media/images/sort_asc_disabled.png",
        "media/images/sort_both.png",
        "media/images/sort_desc.png",
        "media/images/sort_desc_disabled.png",
        "media/js/jquery.dataTables.js",
        "media/js/jquery.dataTables.min.js",
        "package.json"
      ]
    }
  ]
}